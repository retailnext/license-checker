{
  "name": "license-checker",
  "description": "Check license info for a pacakge",
  "author": "Dav Glass <davglass@gmail.com>",
  "version": "2.0.2-nb.1",
  "dependencies": {
    "chalk": "~0.5.1",
    "mkdirp": "^0.3.5",
    "nopt": "^2.2.0",
    "read-installed": "git://github.com/retailnext/read-installed.git#53688097ade57398462d4a5ad1cc9e7d21ae01b3",
    "treeify": "^1.0.1"
  },
  "devDependencies": {
    "yui-lint": "~0.1.1",
